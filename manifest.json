{
    "manifest_version": 2,
    "name": "AutoDraw for skribbl.io",
    "description": "Auto draws images in pictionary game, skribbl.io.",
    "version": "0.2.8",
    "icons": {
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://skribbl.io/*"
            ],
            "css": [
                "main.css"
            ],
            "js": [
                "dist/main.js"
            ]
        }
    ],
    "homepage_url": "https://github.com/galehouse5/skribbl-io-autodraw"
}
